    %store/vec4 v0x62e5b6bd6010_0, 0, 3;
    %pushi/vec4 200, 0, 32;
    %store/vec4 v0x62e5b6bd6110_0, 0, 32;
    %fork TD_tb_washing_machine.wait_for_state, S_0x62e5b6bd5c80;
    %join;
