    %store/vec4 v0x62e5b6bd6350_0, 0, 1;
    %delay 20000, 0;
    %load/vec4 v0x62e5b6bd6620_0;
    %cmpi/ne 1, 0, 2;
    %jmp/0xz  T_14.0, 6;
    %vpi_call 2 190 "$display", "ERROR: Failed to switch to NORMAL cycle" {0 0 0};
